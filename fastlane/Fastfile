default_platform('ios')

platform :ios do

  lane :test do
    remote_scan(
      project: "iosapp/iosapp.xcodeproj",
      devices: ["iPhone 5s"]
    )
  end  

  lane :screenshot do
    remote_snapshot(
      project: "iosapp_with_snapshot/iosapp.xcodeproj",
      devices: ["iPhone 5s"],
      languages: ["en-US"]
    )
  end

end
